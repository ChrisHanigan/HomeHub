{
    "name": "Home Hub: New Tab page bookmarks navigator",
    "version": "0.951",
    "description": "HomeHub replaces your new tab page with a 'hub' for your bookmarks, quicklinks, topsites, history, as well as the weather and date.",
    "permissions": ["geolocation", "topSites", "history", "bookmarks"],
    "chrome_url_overrides" : {"newtab": "homepage.html"},
    "options_page": "options.html",
    "page_action": {
      "default_icon": {
        "16": "images/H16.png",
        "32": "images/H32.png",
        "48": "images/H48.png",
        "128": "images/H128.png"
      }
    },
    "icons": {
      "16": "images/H16.png",
      "32": "images/H32.png",
      "48": "images/H48.png",
      "128": "images/H128.png"
    },
    "manifest_version": 2
  }
