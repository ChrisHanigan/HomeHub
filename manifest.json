{
    "name": "Bookmark Home",
    "version": "0.90",
    "description": "Bookmark homepage",
    "permissions": ["geolocation", "topSites", "history", "storage", "bookmarks"],
    "chrome_url_overrides" : {"newtab": "homepage.html"},
    "options_page": "options.html",
    "page_action": {
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "manifest_version": 2
  }
